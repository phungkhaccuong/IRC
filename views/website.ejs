        <script>
            $(document).ready(function() {
				//the value is used to save data from imput
				var moneyFromForm = 0,periodFromForm = 0, data, nameOfBanks = [];
				/** to check two array have similar elements */
				var containsAll = (arr1, arr2) => arr2.every(item => arr1.includes(item))
				var similarMembers = (arr1, arr2) => containsAll(arr1, arr2) && containsAll(arr2, arr1);
				var colors = [
					{id: 'BANKS', Name: 'Tất cả ngân hàng'},
					{id: 'SHBVN', Name: 'Ngân hàng Shinhan Việt Nam'},
					{id: 'TECHCOMBANK', Name: 'Ngân hàng Kỹ Thương'},
					{id: 'UOB', Name: 'Ngân hàng UOB Việt Nam'},
					{id: 'VIETABANK', Name: 'Ngân hàng Việt Á'},
					{id: 'VIETBANK', Name: 'Ngân hàng Việt Nam Thương Tín'},
					{id: 'VPBANK', Name: 'Ngân hàng Việt Nam Thịnh Vượng'},
					{id: 'WOORIBANK', Name: 'Ngân hàng Woori Việt Nam'},
					{id: 'SCBVLBANK', Name: 'Ngân hàng Standard Chartered Việt Nam'},
					{id: 'SACOMBANK', Name: 'Ngân hàng Sài Gòn Thương Tín'},
					{id: 'MSB', Name: 'Ngân hàng Hàng Hải'},
					{id: 'HSBC', Name: 'Ngân hàng HSBC Việt Nam'},
					{id: 'HDBANK', Name: 'Ngân hàng Phát triển TPHồ Chí Minh'},
					{id: 'COOPBANK', Name: 'Ngân hàng Hợp tác xã Việt Nam'},
					{id: 'CIMB', Name: 'Ngân hàng CIMB Việt Nam'},
					{id: 'ACB', Name: 'Ngân hàng Á Châu'},
					{id: 'BIDV', Name: 'Ngân hàng Đầu tư và Phát triển Việt Nam'},
					{id: 'EXIMBANK', Name: 'Ngân hàng Xuất Nhập Khẩu'},
					{id: 'PGBANK', Name: 'Ngân hàng Xăng dầu Petrolimex'},
					{id: 'VIETCOMBANK', Name: 'Ngân hàng Ngoại Thương Việt Nam'},
					{id: 'SCB', Name: 'Ngân hàng Sài Gòn'},
					{id: 'ABBANK', Name: 'Ngân hàng An Bình'},
					{id: 'AGRIBANK', Name: 'Ngân hàng NN&PT Nông thôn Việt Nam'},
					{id: 'BAOVIETBANK', Name: 'Ngân hàng Bảo Việt'},
					{id: 'CB', Name: 'Ngân hàng Xây dựng'},
					{id: 'IVB', Name: 'Ngân hàng Indovina'},
					{id: 'KIENLONGBANK', Name: 'Ngân hàng Kiên Long'},
					{id: 'NAMABANK', Name: 'Ngân hàng Nam Á'},
					{id: 'OCB', Name: 'Ngân hàng Phương Đông'},
					{id: 'OCEANBANK', Name: 'Ngân hàng Đại Dương'},
					{id: 'PBVN', Name: 'Ngân hàng Public Bank Việt Nam'},
					{id: 'PVCOMBANK', Name: 'Ngân hàng Đại Chúng Việt Nam'},
					{id: 'SAIGONBANK', Name: 'Ngân hàng Sài Gòn Công Thương'},
					{id: 'SHB', Name: 'Ngân hàng Sài Gòn – Hà Nội'},
					{id: 'VIETINBANK', Name: 'Ngân hàng Công thương Việt Nam'},
					{id: 'VRB', Name: 'Ngân hàng Việt – Nga'},
					{id: 'HLBVN', Name: 'Ngân hàng Hong Leong Việt Nam'},
					{id: 'BACABANK', Name: 'Ngân hàng Bắc Á'},
					{id: 'DONGABANK', Name: 'Ngân hàng Đông Á'},
					{id: 'NCB', Name: 'Ngân hàng Quốc dân'},
					{id: 'TPBANK', Name: 'Ngân hàng Tiên Phong'},
					{id: 'VIB', Name: 'Ngân hàng Quốc Tế'}
        		];

				$(function () {
					$("#bank").igCombo({
						width: 300,
						dataSource: colors,
						textKey: "Name",
						valueKey: "id",
						multiSelection: {
							enabled: true,
							showCheckboxes: true
						},
						dropDownOrientation: "bottom"
					});
					

				});

				//check if user click on input form
				$('#money').click(function(){
					var money = $(this).val().toString().replace(/\D/g,'');
					$(this).val(money);
				});

				//check if user enter a value into form
				$('#money').keypress(function(evt){
					var dataOfForm =String.fromCharCode(evt.which);
					if(!(/[0-9]/.test(dataOfForm))) {
						evt.preventDefault();
					}
				});
	
				//check if user out of form
				$('#money').blur(function(){
					var money = $(this).val().toString().replace(/\D/g,'');
					if(money){
						$("#money").val(money.replace(/\B(?=(\d{3})+(?!\d))/g, ",") + " đ");
					}
				});

                $("#searching").click(function(e){
                    //send data to server
                    data = {
                                money: $("#money").val().replace(/\D/g,''),
								period: $("#period").val(),
								banks: $('#bank').igCombo('value')
					}
					//check money user enter into form, if user have not entered yet =>  return
					if(!data.money) {
						alert('Bạn cần nhập số tiền!');
						return;
					}
					//check choosing bank
					if($('#bank').igCombo('value').length === 0){
						alert('Bạn chưa chọn ngân hàng!');
						return;
					}
					//set money vnd
					$("#money").val(data.money.replace(/\B(?=(\d{3})+(?!\d))/g, ",") + " đ");
					data.money = parseInt(data.money);

					if(data.money === moneyFromForm && data.period === periodFromForm && similarMembers(nameOfBanks,data.banks)) {
						return false;
					} else {
						moneyFromForm = data.money;
						periodFromForm = data.period;
						nameOfBanks = data.banks;
						//remove the result if user click more one time
						$("#banks").remove();
						$("#info").remove();
						$('#finhay').remove();
						$('.buttonload').attr("style", "display:block");

						$.ajax({
							url: window.location.pathname,
							method: 'GET',
							dataType: 'html',
							data: data,
							error: function (response) {
								alert("Có lỗi xảy ra: " + response.message);
							},
							success: function (response) {
							if(response) {
								$('.buttonload').attr("style", "display:none");
								$('#rs').html(response);
							}
								
							}
						})

					}
						e.preventDefault();
					}); 
				 //event when user click pagination button
				 $('body').on('click', '.pageUserAsset', function(e) {
					data.page = $(this).attr("value");
					//send data to server
					$.ajax({
						url: window.location.pathname,
						method: 'GET',
						dataType: 'html',
						data: data,
						error: function (response) {
							alert("Có lỗi xảy ra: " + response.message);
						},
						success: function (response) {
							if(response) {
								$('#rs').html(response);
							}
						}
					})
					e.preventDefault();
					
				});
        
            });
        </script>
